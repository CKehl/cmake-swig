<?xml version="1.0" encoding="UTF-8"?>
<project
  xmlns="http://maven.apache.org/POM/4.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <parent>
    <groupId>org.sonatype.oss</groupId>
    <artifactId>oss-parent</artifactId>
    <version>7</version>
  </parent>

  <properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<maven.compiler.source>1.8</maven.compiler.source>
		<maven.compiler.target>1.8</maven.compiler.target>
  </properties>

  <groupId>org.mizux.cmake-swig</groupId>
  <artifactId>cmake-swig-java</artifactId>
  <packaging>pom</packaging>
  <version>@PROJECT_VERSION@</version>
  <name>CMakeSwig (Parent)</name>
  <description>A Modern CMake Swig C++/Java project.</description>
  <licenses>
    <license>
      <name>The Apache License, Version 2.0</name>
      <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
    </license>
  </licenses>
  <url>https://github.com/mizux/cmake-swig</url>

  <developers>
		<developer>
			<name>Mizux Seiha</name>
      <organization>Mizux</organization>
      <organizationUrl>http://www.mizux.net</organizationUrl>
    </developer>
  </developers>

  <scm>
    <url>http://github.com/mizux/cmake-swig/</url>
    <connection>scm:git:git://github.com/mizux/cmake-swig.git</connection>
    <developerConnection>scm:git:ssh://git@github.com/mizux/cmake-swig.git</developerConnection>
    <tag>HEAD</tag>
  </scm>

  <issueManagement>
    <system>GitHub Issues</system>
    <url>http://github.com/mizux/cmake-swig/issues</url>
  </issueManagement>

  <distributionManagement>
    <repository>
      <id>sonatype-nexus-staging</id>
      <name>Sonatype Nexus Staging</name>
      <url>https://oss.sonatype.org/service/local/staging/deploy/maven2</url>
    </repository>
    <snapshotRepository>
      <id>sonatype-nexus-snapshots</id>
      <name>Sonatype Nexus Snapshots</name>
      <url>http://oss.sonatype.org/content/repositories/snapshots</url>
    </snapshotRepository>
  </distributionManagement>
</project>
